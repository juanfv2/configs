{
  "Laravel Print to laravel.log": {
    "prefix": ["pv"],
    "body": ["logger(__FILE__ . ':' . __LINE__ . ' $1 ', [$1]);$0"],
    "description": "Laravel Print to laravel.log"
  },
  "Laravel db listen": {
    "prefix": ["db-l"],
    "body": [
      "\\Illuminate\\Support\\Facades\\DB::listen(function (\\$query) {",
      " dump(\\$query->sql);",
      " dump(\\$query->bindings);",
      " // dump(\\$query->time);",
      ");"
    ],
    "description": "Laravel add foreign"
  },
  "Laravel add foreign": {
    "prefix": ["s-f"],
    "body": ["\\$table->foreignId('$1')${2:->nullable()}->constrained(${3:'$4'})${5:->onDelete('${6:cascade}')};$0"],
    "description": "Laravel add foreign"
  },
  "foreach": {
    "prefix": ["fore"],
    "body": ["foreach(${1:iterable} as ${2:value}) {", "    ${3:your-code}", "}"],
    "description": "foreach"
  },
  "foreach key-value": {
    "prefix": ["forek"],
    "body": ["foreach(${1:iterable} as ${2:key} => ${3:value}) {", "    ${4:your-code}", "}"],
    "description": "foreach key-value"
  },
  "public function": {
    "prefix": ["pubf"],
    "body": ["public function ${1:FunctionName}($2)", "{", "  ${3:// your-code}", "}"],
    "description": "public function"
  },
  "private function": {
    "prefix": ["prif"],
    "body": ["private function ${1:FunctionName}($2)", "{", "  // ${3:your-code}", "}"],
    "description": "private function"
  },
  "public function for test": {
    "prefix": ["test"],
    "body": ["/** @test */", "public function ${1:FunctionName}()", "{", "  ${2:// your-code}", "}"],
    "description": "public function"
  }
}
